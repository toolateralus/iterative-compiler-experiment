; ModuleID = 'program'
source_filename = "program"

@str = private unnamed_addr constant [5 x i8] c"%d\\n\00", align 1

declare void @printf(ptr, void, ...)

define void @main() {
entry:
  %n = alloca i32, align 4
  store i32 100, ptr %n, align 4
  %n1 = load ptr, ptr %n, align 8
  %call = call void (ptr, void, ...) @printf(ptr @str, ptr %n1)
  ret void
}
