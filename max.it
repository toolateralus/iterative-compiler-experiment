fn main() @entry {
  hello_world();
}

fn hello_world() {
  Vector_3 dependent; # ALLOC
  Vector_2 n = dependent.xy; # ASSIGN, GEP
  dependent.z = 100; # ASSIGN, SEP
  printf("dependent.z = '%d'\n", dependent.z);
  printf("Hello, World!\n");
}

type Vector_3(Vector_2 xy, i32 z);

type Vector_2(i32 x, i32 y);

type Matrix_4x4(Vector_4 a, Vector_4 b, Vector_4 c, Vector_4 d);

type Vector_4(i32 x, i32 y, i32 z, i32 w);

fn printf(String, ...) @extern;